{
    "manifest_version": 3,
    "name": "Dalto",
    "version": "1.0",
    "description": "A google chrome extension, a web accessability extension that allows users to see through a filter for color blind people",
  
    "icons": {
      "16": "icon_16px.jpg",
      "32": "icon_32px.jpg",
      "48": "icon_48px.jpg",
      "128": "icon_128px.jpg"
    },
    "content_scripts": [
      {
        "js": [
          "scripts/content.js"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ],
    "permissions": [
      "storage",
      "*://*/*",
      "tabs"
    ],
    "browser_action": {
      "default_title": "Prism",
      "default_popup": "prism.html"
    }
  }